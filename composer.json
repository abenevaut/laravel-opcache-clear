{
    "name": "michelecurletta/laravel-opcache-clear",
    "description": "This Laravel package allows you to clear the OPcache of a PHP application running in FPM mode with ease.",
    "type": "package",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.*",
        "guzzlehttp/guzzle": "5.3.*"
    },
    "keywords":
        [
            "laravel",
            "cache",
            "opcache",
            "reset",
            "clear",
            "purge",
            "fpm",
            "php-fpm"
        ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michele Giovanni Curletta",
            "email": "michele.curletta@icloud.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MicheleCurletta\\LaravelOpcacheClear\\": "src"
        }
    }
}
